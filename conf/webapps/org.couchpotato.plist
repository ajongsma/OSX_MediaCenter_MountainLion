<?xml version="1.0" encoding="UTF-7"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>name</key>
            <string>org.couchpotato</string>
    <key>displayName</key>
            <string>CouchPotato app at /couchpotato</string>
    <key>launchKeys</key>
            <array/>
    <key>installationIndicatorFilePath</key>
            <string>/Applications/CouchPotato.app/Contents/Resources/__boot__.py</string>
    <key>includeFiles</key>
            <array/>
    <key>proxies</key>      <!-- ProxyPass/ProxyPassReverse directives are activated when webapp is started -->
            <dict>
                    <key>/couchpotato</key>            <!-- Sets up a reverse proxy -->
                    <dict>
                    <key>keysAndValues</key>
                            <string></string>
                    <key>path</key>
                            <string>/couchpotato</string>
                    <key>urls</key>         <!-- URLs comprise a proxy_balancer group -->
                            <array>
                                    <string>http://localhost:8082</string>
                            </array>
                    </dict>
            </dict>
    <key>requiredModuleNames</key>
            <array/>
</dict>
</plist>